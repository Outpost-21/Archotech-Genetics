<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<GeneDef>
		<defName>ArchoGenes_ArchotechSkin</defName>
		<label>archoskin</label>
		<labelShortAdj>archo-skinned</labelShortAdj>
		<description>Carriers of this gene have a rigid but smooth skin, with strange lightly glowing lines along seemingly random sections of their bodies, even their hair has a vibrant slight glow to it.</description>
		<biostatCpx>1</biostatCpx>
		<biostatArc>1</biostatArc>
		<displayCategory>Archite</displayCategory>
		<displayOrderInCategory>6900</displayOrderInCategory>
		<iconPath>ArchoGenetics/GeneIcons/Gene_ArchoSkin</iconPath>
		<forcedHeadTypes>
			<li>Male_AverageNormal</li>
			<li>Male_AveragePointy</li>
			<li>Male_AverageWide</li>
			<li>Female_AverageNormal</li>
			<li>Female_AveragePointy</li>
			<li>Female_AverageWide</li>
		</forcedHeadTypes>
		<exclusionTags>
			<li>Fur</li>
		</exclusionTags>
		<selectionWeight>0</selectionWeight>
		<statOffsets>
			<ArmorRating_Blunt>1.5</ArmorRating_Blunt>
			<ArmorRating_Sharp>1.5</ArmorRating_Sharp>
			<ArmorRating_Heat>1.5</ArmorRating_Heat>
		</statOffsets>
		<fur>ArchoGenes_ArchotechSkin</fur>
		<skinIsHairColor>false</skinIsHairColor>
		<tattoosVisible>true</tattoosVisible>
		<renderNodeProperties>
			<li>
				<debugLabel>ArchotechSkin</debugLabel>
				<nodeClass>TabulaRasa.PawnRenderNode_FurCustom</nodeClass>
				<workerClass>PawnRenderNodeWorker_Fur</workerClass>
				<baseLayer>5</baseLayer>
				<rotDrawMode>Fresh, Rotting</rotDrawMode>
				<colorType>Custom</colorType>
				<color>(255, 255, 255)</color>
				<parentTagDef>Body</parentTagDef>
			</li>
		</renderNodeProperties>
	</GeneDef>

